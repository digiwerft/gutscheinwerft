{
    "name": "digiwerft/gutscheinwerft",
    "type": "contao-bundle",
    "license": "MIT",
    "description": "Integrates the \"Gutscheinwerft\" shop into Contao",
    "authors": [
        {
            "name": "Ivo Schimani",
            "email": "ivo.schimani@digiwerft.de",
            "homepage": "https://www.digiwerft.de",
            "role": "Project leader"
        }
    ],
    "keywords": [
        "contao",
        "gutscheinwerft",
        "voucher",
        "gutscheine"
    ],
    "require": {
        "php": "^8.2",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Digiwerft\\Gutscheinwerft\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Digiwerft\\Gutscheinwerft\\ContaoManager\\Plugin"
    }
}